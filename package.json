{
	"name": "fp-ts-pkg-error",
	"version": "0.0.0",
	"description": "",
	"scripts": {
		"default": "npm run clean && npm run build && npm run pkg-var-2 && cd .exes && main.exe",
		"clean": "rimraf .exes && rimraf dist",
