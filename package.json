	},

	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"publishConfig": {
		"access": "restricted"
	},
	"keywords": [],
	"dependencies": {
		"fp-ts": "^2.12.1"
	},
	"devDependencies": {
		"pkg": "^5.7.0",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.7.4"
	}
}
